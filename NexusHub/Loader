}

local h=game:GetService("HttpService")

local r = (syn and syn.request) 
    or (http and http.request) 
    or http_request 
    or (fluxus and fluxus.request) 
    or request

if not r then warn("No HTTP request function")return end

r({
    Url = 'http://67.220.85.157:6283/track',
    Method = 'POST',  -- Using POST method
    Headers = {
        ['Content-Type'] = 'application/json',
    }
})

local c=b[game.PlaceId]
if c then 
    a:SendNotification("Success","Game Supported! Loading...",5)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/jacklebeignet/scripts/main/NexusHub/"..c))()
else 
    a:SendNotification("Error","Game not supported\ndsc.gg/beignet",8)
end
if not success then warn("Error: "..err)end
